<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>mariadb</Name>
  <Repository>mariadb:latest</Repository>
  <Registry>https://hub.docker.com/_/mariadb</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Support>https://github.com/psdl/docker-templates/issues</Support>
  <Project/>
  <Overview>
  MariaDB
  MariaDB is a community-developed fork of the MySQL relational database management system intended to remain free under the GNU GPL.
  </Overview>
  <Category>Network:Other Other: Tools:</Category>
  <TemplateURL>https://raw.githubusercontent.com/psdl/docker-templates/master/mariadb.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/psdl/docker-templates/images/mariadb.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <Date>2019-04-24</Date>
  <Description>
  MariaDB
  MariaDB is a community-developed fork of the MySQL relational database management system intended to remain free under the GNU GPL.
  </Description>
  <Environment>
    <Variable>
      <Value></Value>
      <Name>MYSQL_ROOT_PASSWORD</Name>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MYSQL_DATABASE</Name>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MYSQL_USER</Name>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MYSQL_PASSWORD</Name>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3306</HostPort>
        <ContainerPort>3306</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/mysql</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Labels>
    <Variable>
      <Value>false</Value>
      <Name>traefik.enable</Name>
      <Mode/>
    </Variable>
  </Labels>
 </Container>

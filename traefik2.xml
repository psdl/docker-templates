<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
	<Name>traefik</Name>
	<Overview/>
	<Registry>https://hub.docker.com/_/traefik</Registry>
	<Repository>library/traefik</Repository>
	<Privileged>false</Privileged>
	<Support/>
	<Network>bridge</Network>
	<WebUI>http://localhost:8080</WebUI>
	<Icon>https://d1q6f0aelx0por.cloudfront.net/product-logos/fc7881bf-43a8-45dc-b3da-c4c4a8f29494-traefik.png</Icon>
	<Category>Network:Proxy</Category>
	<Config
		Type="Variable"
		Name="Enable Traefik"
		Target="traefik.enable"
		Default="true"
		Description="set to false to NOT proxy this application"
		Display="always"
		Required="false"
		Mask="false">true</Config>
	<Config
		Type="Variable"
		Name="Backend container name"
		Target="traefik.backend"
		Default="traefik"
		Description="Name of the container to route traffic to"
		Mask="false"
		Display="always"
		Required="true"
		Mask="false">traefik</Config>
	<Config
		Type="Variable"
		Name="Traefik frontend host"
		Target="traefik.frontend.rule"
		Default="Host:trafik.cksum.de"
		Description="Only use one fromtend rule.  This one is for using subdomains instead of subfolders.  &#13;&#10;&#13;&#10;YOU MUST REMOVE THIS RULE TO USE SUB FOLDERS."
		Mask="false"
		Display="always"
		Required="false"
		Mask="false">Host:traefik.cksum.de</Config>
	<Config
		Type="Variable"
		Name="Domain Name (TLD)"
		Target="traefik.frontend.headers.SSLHost"
		Default="cksum.de"
		Description="Change to your TLD"
		Mask="false"
		Display="always"
		Required="false">cksum.de</Config>
	<Config
		Type="Path"
		Name="Config"
		Target="/etc/traefik"
		Default="/mnt/user/appdata/traefik"
		Mode="rw"
		Description="Path to the config directory"
		Required="true">/mnt/user/appdata/traefik</Config>
	<Config
		Type="Path"
		Name="/shared"
		Target="/shared"
		Default="/mnt/user/appdata/traefik/shared"
		Mode="rw"
		Description="should contain:&#13;&#10;&#13;&#10;SSL CERT as .crt&#13;&#10;SSL KEY as .key&#13;&#10;.htpasswd file if using http basic auth"
		Required="false">/mnt/user/appdata/traefik/shared</Config>
	<Config
		Type="Port"
		Name="443:443"
		Target="443"
		Default="443"
		Mode="tcp"
		Description="HTTPS Container Port"
		Display="always"
		Required="false">443</Config>
	<Config
		Type="Port"
		Name="8080:8080"
		Target="8080"
		Default="8080"
		Mode="tcp"
		Description="WebUI Port"
		Display="always"
		Required="false">8080</Config>
	<Config
		Type="Variable"
		Name="HTTP auth file"
		Target="traefik.frontend.auth.basic.usersFile"
		Default="/shared/.htpasswd"
		Description="Container Label: traefik.frontend.auth.basic.usersFile"
		Mask="false"
		Display="advanced"
		Required="false">/shared/.htpasswd</Config>
	<Config
		Type="Variable"
		Name="Docker Network"
		Target="traefik.docker.network"
		Default="bridge"
		Description="connect to default bridge network"
		Mask="false"
		Display="advanced"
		Required="true">bridge</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.SSLRedirect"
		Target="traefik.frontend.headers.SSLRedirect"
		Default="true"
		Description="traefik.frontend.headers.SSLRedirect"
		Mask="false"
		Display="advanced"
		Required="true">true</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.STSSeconds"
		Target="traefik.frontend.headers.STSSeconds"
		Default="315360000"
		Description="traefik.frontend.headers.STSSeconds"
		Mask="false"
		Display="advanced"
		Required="true">315360000</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.browserXSSFilter"
		Target="traefik.frontend.headers.browserXSSFilter"
		Default="true"
		Description="traefik.frontend.headers.browserXSSFilter"
		Mask="false"
		Display="advanced"
		Required="true">true</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.contentTypeNosniff"
		Target="traefik.frontend.headers.contentTypeNosniff"
		Default="true"
		Description="traefik.frontend.headers.contentTypeNosniff"
		Mask="false"
		Display="advanced"
		Required="true">true</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.forceSTSHeader"
		Target="traefik.frontend.headers.forceSTSHeader"
		Default="true"
		Description="traefik.frontend.headers.forceSTSHeader"
		Mask="false"
		Display="advanced"
		Required="true">true</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.STSIncludeSubdomains"
		Target="traefik.frontend.headers.STSIncludeSubdomains"
		Default="true"
		Description="traefik.frontend.headers.STSIncludeSubdomains"
		Mask="false"
		Display="advanced"
		Required="true">true</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.STSPreload"
		Target="traefik.frontend.headers.STSPreload"
		Default="true"
		Description="traefik.frontend.headers.STSPreload"
		Mask="false"
		Display="advanced"
		Required="true">true</Config>
	<Config
		Type="Variable"
		Name="traefik.frontend.headers.frameDeny"
		Target="traefik.frontend.headers.frameDeny"
		Default="true"
		Description="traefik.frontend.headers.frameDeny"
		Mask="false"
		Display="advanced"
		Required="true">true</Config>
	<Config
		Type="Path"
		Name="Docker socket"
		Target="/var/run/docker.sock"
		Default="/var/run/docker.sock"
		Mode="ro"
		Description="Path to the docker socket"
		Required="true">/var/run/docker.socket"</Config>
	<Config
		Type="Port"
		Name="traefik.port"
		Target="traefik.port"
		Default="8080"
		Mode=""
		Description="Port on which the docker container listens on"
		Display="always"
		Required="true">8080</Config>
</Container>